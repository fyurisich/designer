                NUMERIC .T.
                COL 75
                HEIGHT 18
                MAXLENGTH 11
                ROW 35
                WIDTH 89
            END TEXTBOX

            DEFINE LABEL lbl_Usuario
                VALUE "Usuário :"
                RIGHTALIGN .T.
                COL 15
                HEIGHT 18
                ROW 58
                WIDTH 57
            END LABEL

            DEFINE TEXTBOX oUsuario
                COL 75
                HEIGHT 18
                MAXLENGTH 30
                ROW 58
                WIDTH 241
            END TEXTBOX

            DEFINE LABEL lbl_Nome
                VALUE "Nome :"
                RIGHTALIGN .T.
                COL 15
                HEIGHT 18
                ROW 81
                WIDTH 57
            END LABEL

            DEFINE TEXTBOX oNome
                COL 75
                HEIGHT 18
                MAXLENGTH 60
                ROW 81
                WIDTH 501
            END TEXTBOX

            DEFINE LABEL lbl_Password
                VALUE "Password :"
                RIGHTALIGN .T.
                COL 15
                HEIGHT 18
                ROW 104
                WIDTH 57
