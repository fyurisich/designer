                COL 75
                HEIGHT 18
                MAXLENGTH 32
                ROW 104
                WIDTH 257
            END TEXTBOX

            DEFINE CHECKBOX oStatus
                CAPTION "Ativo ?"
                COL 75
                HEIGHT 18
                ROW 127
                WIDTH 50
            END CHECKBOX

            DEFINE LABEL lbl_Nivel
                VALUE "Nivel :"
                RIGHTALIGN .T.
                COL 15
                HEIGHT 18
                ROW 150
                WIDTH 57
            END LABEL

            DEFINE COMBOBOXEX oNivel
                ITEMS { "Operador" , "Manutenção" , "Gerencial" }
