DEFINE WINDOW TEMPLATE AT 189, 407 WIDTH 376 HEIGHT 297  TITLE "" MODAL NOSIZE ON INIT (NovoUser_Oninit())

    DEFINE LABEL LCodigo
        COL 10
        HEIGHT 30
        ROW 22
        VALUE "Código"
        WIDTH 50
    END LABEL

    DEFINE TEXTBOX TxtCODIGO
        COL 60
        HEIGHT 25
        ROW 20
        WIDTH 90
    END TEXTBOX

    DEFINE LABEL LNivel
        COL 283
        HEIGHT 25
        ROW 22
        VALUE "Nível"
        WIDTH 50
    END LABEL

    DEFINE TEXTBOX TxtNIVEL
        COL 332
        HEIGHT 25
        MAXLENGTH 1
        ONENTER (Iif( NovoUser.TxtNIVEL.Value $ "1234567890" ,  NovoUser.TxtNOME.SetFocus , NovoUser.TxtNIVEL.SetFocus ))
        ROW 20
        TOOLTIP "Digite o Nível do Usuário para o Sistema  0  a 9"
        WIDTH 20
    END TEXTBOX

    DEFINE LABEL LNome
        COL 10
        HEIGHT 25
        ROW 84
        VALUE "Nome"
        WIDTH 50
