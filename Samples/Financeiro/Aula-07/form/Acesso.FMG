    DEFINE BUTTON Bt_Logoff
        ACTION (Main.Release)
        CAPTION "&Cancela"
        COL 143
        FLAT TRUE
        FONTNAME "MS Sans Serif"
        FONTSIZE 9
        HEIGHT 28
        ROW 90
        WIDTH 100
    END BUTTON

END WINDOW

