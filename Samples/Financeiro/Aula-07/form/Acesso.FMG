        ROW 48
        TOOLTIP "Senha de Acesso"
        UPPERCASE TRUE
        WIDTH 120
    END TEXTBOX

    DEFINE BUTTON Bt_Login
        ACTION (Acesso_Bt_Login_Action())
        CAPTION "&Login"
        COL 30
        FLAT TRUE
        FONTNAME "MS Sans Serif"
        FONTSIZE 9
        HEIGHT 28
        ROW 90
        WIDTH 100
