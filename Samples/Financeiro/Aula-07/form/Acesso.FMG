DEFINE WINDOW TEMPLATE AT 185, 318 WIDTH 280 HEIGHT 160  TITLE "Acesso ao Sistema" MODAL NOSYSMENU BACKCOLOR { 0 , 0 , 255 }

    DEFINE LABEL Label_User
        BACKCOLOR { 0 , 0 , 255 }
        COL 30
        FONTBOLD TRUE
        FONTCOLOR { 255 , 255 , 255 }
        FONTNAME "Arial"
        FONTSIZE 9
        HEIGHT 35
        ROW 10
        VALUE "Usuário"
        WIDTH 120
    END LABEL

    DEFINE LABEL Label_Password
        BACKCOLOR { 0 , 0 , 255 }
        COL 30
        FONTBOLD TRUE
        FONTCOLOR { 255 , 255 , 255 }
        FONTNAME "Arial"
        FONTSIZE 9
        HEIGHT 35
        ROW 45
        VALUE "Senha"
        WIDTH 120
    END LABEL

    DEFINE TEXTBOX p_User
        COL 120
        FONTNAME "Arial"
        FONTSIZE 9
        HEIGHT 25
        MAXLENGTH 10
        ONENTER (Acesso_p_User_Onenter())
        ROW 13
        UPPERCASE TRUE
        WIDTH 120
    END TEXTBOX

    DEFINE TEXTBOX p_password
        COL 120
        FONTNAME "Arial"
        FONTSIZE 9
        HEIGHT 24
        MAXLENGTH 5
        ONENTER (Acesso_p_password_Onenter())
