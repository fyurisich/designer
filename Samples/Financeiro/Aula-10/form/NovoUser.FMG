    DEFINE BUTTON Excluir
        ACTION (NovoUser_Excluir_Action())
        CAPTION "&Excluir"
        COL 90
        FLAT TRUE
        HEIGHT 25
        ROW 210
        WIDTH 70
    END BUTTON

    DEFINE BUTTON BCancelar
        ACTION (NovoUser.Release)
        CAPTION "&Cancelar"
        COL 290
        FLAT TRUE
        HEIGHT 25
        ROW 210
        WIDTH 70
    END BUTTON

    DEFINE CHECKBOX oSenha
        CAPTION "Voltar senha do usuário para o Padrão"
        COL 137
        HEIGHT 18
        ROW 55
        WIDTH 219
    END CHECKBOX

    DEFINE STATUSBAR FONT "Arial" SIZE 10
        STATUSITEM "Manutenção no" 
    END STATUSBAR
